{
  "name": "up-your-ass-ets",
  "version": "1.0.0",
  "description": "node upload assets server",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src/ --out-dir ./dist/",
    "lint": "./node_modules/.bin/eslint src/index.js",
    "start": "npm run lint && npm run build && node --es_staging dist/index.js",
    "debug": "npm run lint && npm run build && ./node_modules/.bin/node-debug --nodejs --es_staging dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goncalvesjoao/up-your-ass-ets.git"
  },
  "keywords": [
    "node",
    "upload",
    "assets"
  ],
  "author": "goncalvesjoao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goncalvesjoao/up-your-ass-ets/issues"
  },
  "homepage": "https://github.com/goncalvesjoao/up-your-ass-ets#readme",
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-1": "6.3.13",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "node-inspector": "^0.12.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "express": "4.13.4",
    "formidable": "^1.0.17"
  }
}
